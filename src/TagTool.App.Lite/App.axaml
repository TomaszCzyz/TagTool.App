<Application
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
    xmlns:local="using:TagTool.App.Lite"
    x:Class="TagTool.App.Lite.App"
    RequestedThemeVariant="Dark">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key='Light'>
                    <SolidColorBrush x:Key='Primary'>#415f91</SolidColorBrush>
                    <SolidColorBrush x:Key='PrimaryInverse'>#aac7ff</SolidColorBrush>
                    <SolidColorBrush x:Key='OnPrimary'>#ffffff</SolidColorBrush>
                    <SolidColorBrush x:Key='PrimaryContainer'>#d6e3ff</SolidColorBrush>
                    <SolidColorBrush x:Key='OnPrimaryContainer'>#001b3e</SolidColorBrush>
                    <SolidColorBrush x:Key='Secondary'>#565f71</SolidColorBrush>
                    <SolidColorBrush x:Key='OnSecondary'>#ffffff</SolidColorBrush>
                    <SolidColorBrush x:Key='SecondaryContainer'>#dae2f9</SolidColorBrush>
                    <SolidColorBrush x:Key='OnSecondaryContainer'>#131c2b</SolidColorBrush>
                    <SolidColorBrush x:Key='Tertiary'>#705575</SolidColorBrush>
                    <SolidColorBrush x:Key='OnTertiary'>#ffffff</SolidColorBrush>
                    <SolidColorBrush x:Key='TertiaryContainer'>#fad8fd</SolidColorBrush>
                    <SolidColorBrush x:Key='OnTertiaryContainer'>#28132e</SolidColorBrush>
                    <SolidColorBrush x:Key='Error'>#ba1a1a</SolidColorBrush>
                    <SolidColorBrush x:Key='OnError'>#ffffff</SolidColorBrush>
                    <SolidColorBrush x:Key='ErrorContainer'>#ffdad6</SolidColorBrush>
                    <SolidColorBrush x:Key='OnErrorContainer'>#410002</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceDim'>#d9d9e0</SolidColorBrush>
                    <SolidColorBrush x:Key='Surface'>#f9f9ff</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceBright'>#f9f9ff</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceInverse'>#2e3036</SolidColorBrush>
                    <SolidColorBrush x:Key='OnSurfaceInverse'>#f0f0f7</SolidColorBrush>
                    <SolidColorBrush x:Key='OnSurface'>#191c20</SolidColorBrush>
                    <SolidColorBrush x:Key='OnSurfaceVariant'>#44474e</SolidColorBrush>
                    <SolidColorBrush x:Key='Outline'>#74777f</SolidColorBrush>
                    <SolidColorBrush x:Key='OutlineVariant'>#c4c6d0</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceContainerLowest'>#ffffff</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceContainerLow'>#f3f3fa</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceContainer'>#ededf4</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceContainerHigh'>#e7e8ee</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceContainerHighest'>#e2e2e9</SolidColorBrush>
                </ResourceDictionary>
                <ResourceDictionary x:Key='Dark'>
                    <SolidColorBrush x:Key='Primary'>#aac7ff</SolidColorBrush>
                    <SolidColorBrush x:Key='OnPrimary'>#0a305f</SolidColorBrush>
                    <SolidColorBrush x:Key='PrimaryContainer'>#284777</SolidColorBrush>
                    <SolidColorBrush x:Key='PrimaryInverse'>#415f91</SolidColorBrush>
                    <SolidColorBrush x:Key='OnPrimaryContainer'>#d6e3ff</SolidColorBrush>
                    <SolidColorBrush x:Key='Secondary'>#bec6dc</SolidColorBrush>
                    <SolidColorBrush x:Key='OnSecondary'>#283141</SolidColorBrush>
                    <SolidColorBrush x:Key='SecondaryContainer'>#3e4759</SolidColorBrush>
                    <SolidColorBrush x:Key='OnSecondaryContainer'>#dae2f9</SolidColorBrush>
                    <SolidColorBrush x:Key='Tertiary'>#ddbce0</SolidColorBrush>
