                    <SolidColorBrush x:Key='OnSurface'>#e2e2e9</SolidColorBrush>
                    <SolidColorBrush x:Key='OnSurfaceVariant'>#c4c6d0</SolidColorBrush>
                    <SolidColorBrush x:Key='Outline'>#8e9099</SolidColorBrush>
                    <SolidColorBrush x:Key='OutlineVariant'>#44474e</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceContainerLowest'>#0c0e13</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceContainerLow'>#191c20</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceContainer'>#1d2024</SolidColorBrush>
                    <SolidColorBrush x:Key='SurfaceContainerHigh'>#282a2f</SolidColorBrush>
